<script>
    function onloadF() {
        document.body.style.display = 'inline-block';
        document.body.offsetHeight; // no need to store this anywhere, the reference is enough
        document.body.style.display = '';
        document.body.classList.remove('loaded');
        /*if(typeof header!=='undefined'&&header.length){
				document.body.style.paddingTop=header.clientHeight+'px';
			} else {
				document.body.style.paddingTop=document.body.querySelector('header').clientHeight+'px';
			}*/
    };
    setTimeout(onloadF, 500);

    window.addEventListener('load', function () {
        $(document).on('click', '.js-regModal', function (event) {
            event.preventDefault();
            // записываем id цели из аттрибута href
            var href = '#registerModal',
                type = this.dataset.type,
                btn = $(this);
            if (typeof initialModalId === 'undefined') {
                initialModalId = href;
                // modalId=href;
            }
            // закрываем любую открытую модалку
            $.magnificPopup.close();
            // открываем новую
            $.magnificPopup.open({
                // type: 'inline',
                items: {
                    src: href, // can be a HTML string, jQuery object, or CSS selector
                    type: 'inline'
                },
                callbacks: {
                    open: function () {
                        this.content[0].querySelector('[name="hb_author_signup[packetPlain]"]').value = type;
                    },
                },
                preloader: false,
                // fixedContentPos: false,
                mainClass: 'funnel_popup',
                closeOnBgClick: true,
            });
        });

        $(document).on('submit', '[name=hb_author_signup]', function (event) {
            event.preventDefault();

            var form = $(this),
				formData = form.serialize(),
				trackParams = {
					Email: form.find('#hb_author_signup_email').val(),
					Phone: form.find('#hb_author_signup_phone').val()
				};

            $.ajax({
                type: "POST",
                url: '{{ path('hb.author.sign_up') }}',
                data: formData,
                dataType: "json",
                success: function () {
					if(typeof analytics!=='undefined'&&typeof trackParams!=='undefined'){
						analytics.track('Регистрация в админпанель', trackParams);
						setTimeout(function(){
							document.location = '{{ path('hb.courses.list') }}';
						}, 500)
					} else {
						document.location = '{{ path('hb.courses.list') }}';
					}
                },
                error: function (response) {
                    $('div.sign_up_div').html(response.responseText);
                }
            });

            return false;
        });


    })

</script>
